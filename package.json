{
  "name": "darts",
  "version": "1.0.0",
  "description": "Darts Scoreboard",
  "main": "index.ts",
  "scripts": {
    "test": "jest --watch",
    "test:c": "jest --coverage",
    "start": "webpack-dev-server --config webpack.config.dev.js --port 4001 --open --hot --inline",
    "build": "webpack --mode production --config webpack.config.prod.js"
  },
  "author": "Jeff Roy",
  "license": "ISC",
  "dependencies": {
    "mobx": "^4.2.0",
    "mobx-react": "^5.0.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^22.2.3",
    "@types/react": "16.3.13",
    "@types/react-dom": "16.0.5",
    "@types/react-router": "^4.0.24",
    "@types/react-router-dom": "^4.2.6",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "html-webpack-plugin": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "ts-jest": "^22.4.2",
    "ts-loader": "^4.1.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.6.0",
    "tslint-react": "^3.5.1",
    "typescript": "^2.8.1",
    "webpack": "^4.4.1",
    "webpack-bundle-analyzer": "^2.13.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./support/setupTests.js"
    ]
  }
}
